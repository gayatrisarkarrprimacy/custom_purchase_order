<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="seq_purchase_order_domestic" model="ir.sequence">
        <field name="name">Domestic Purchase Order</field>
        <field name="code">purchase.order.domestic</field>
        <field name="prefix">PO/DOM/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_purchase_order_import" model="ir.sequence">
        <field name="name">Imported Purchase Order</field>
        <field name="code">purchase.order.import</field>
        <field name="prefix">PO/IMP/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
